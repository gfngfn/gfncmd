%
\def\omitlistRlfqtfr@defcs{\lfpv\lflnot}
\def\mndtlistRlfqtfr@defcs{\lfqtfr\lfland\lflor\lflimpl\lflimpleqv}
\def\omitlistRlflnot@defcs{\lfpv\lflnot\lfqtfr}
\def\mndtlistRlflnot@defcs{\lfland\lflor\lflimpl\lflimpleqv}
\def\omitlistRlfland@defcs{\lfpv\lflnot\lfland}
\def\mndtlistRlfland@defcs{\lfqtfr\lflor\lflimpl\lflimpleqv}
\def\omitlistRlflor@defcs{\lfpv\lflnot\lflor}
\def\mndtlistRlflor@defcs{\lfqtfr\lfland\lflimpl\lflimpleqv}
\def\omitlistRlflimpl@defcs{\lfpv\lflnot\lfland\lflor\lflimpl}
\def\mndtlistRlflimpl@defcs{\lfqtfr\lflimpleqv}
\def\omitlistRlflimpleqv@defcs{\lfpv\lflnot\lfland\lflor}
\def\mndtlistRlflimpleqv@defcs{\lfqtfr\lflimpl\lflimpleqv}
%
\def\makerightparenomittable#1#2{%
  \expandafter\def\expandafter\added@defcs\expandafter{\csname#2\endcsname}%
%
  \expandafter\let\expandafter\tempomitlist@letcs\expandafter=\csname omitlistR#1@defcs\endcsname
  \addtokentolist{\added@defcs}{\tempomitlist@letcs}%
  \expandafter\let\csname omitlistR#1@defcs\endcsname=\tempomitlist@letcs
%
  \expandafter\let\expandafter\tempmndtlist@letcs\expandafter=\csname mndtlistR#1@defcs\endcsname
  \deletetokenfromlist{\added@defcs}{\tempmndtlist@letcs}%
  \expandafter\let\csname mndtlistR#1@defcs\endcsname=\tempmndtlist@letcs
}
\def\makeleftparenomittable#1#2{%
  \expandafter\def\expandafter\added@defcs\expandafter{\csname#2\endcsname}%
%
  \expandafter\let\expandafter\tempomitlist@letcs\expandafter=\csname omitlistL#1@defcs\endcsname
  \addtokentolist{\added@defcs}{\tempomitlist@letcs}%
  \expandafter\let\csname omitlistL#1@defcs\endcsname=\tempomitlist@letcs
%
  \expandafter\let\expandafter\tempmndtlist@letcs\expandafter=\csname mndtlistL#1@defcs\endcsname
  \deletetokenfromlist{\added@defcs}{\tempmndtlist@letcs}%
  \expandafter\let\csname mndtlistL#1@defcs\endcsname=\tempmndtlist@letcs
}
\def\makerightparenmandatory#1#2{%
  \expandafter\def\expandafter\deleted@defcs\expandafter{\csname#2\endcsname}%
%
  \expandafter\let\expandafter\tempomitlist@letcs\expandafter=\csname omitlistR#1@defcs\endcsname
  \deletetokenfromlist{\deleted@defcs}{\tempomitlist@letcs}%
  \expandafter\let\csname omitlistR#1@defcs\endcsname=\tempomitlist@letcs
%
  \expandafter\let\expandafter\tempmndtlist@letcs\expandafter=\csname omitlistR#1@defcs\endcsname
  \addtokentolist{\deleted@defcs}{\tempmndtlist@letcs}%
  \expandafter\let\csname mndtlistR#1@defcs\endcsname=\tempmndtlist@letcs
}
\def\makeleftparenmandatory#1#2{%
  \expandafter\def\expandafter\deleted@defcs\expandafter{\csname#2\endcsname}%
%
  \expandafter\let\expandafter\tempomitlist@letcs\expandafter=\csname omitlistL#1@defcs\endcsname
  \deletetokenfromlist{\deleted@defcs}{\tempomitlist@letcs}%
  \expandafter\let\csname omitlistL#1@defcs\endcsname=\tempomitlist@letcs
%
  \expandafter\let\expandafter\tempmndtlist@letcs\expandafter=\csname omitlistL#1@defcs\endcsname
  \addtokentolist{\deleted@defcs}{\tempmndtlist@letcs}%
  \expandafter\let\csname mndtlistL#1@defcs\endcsname=\tempmndtlist@letcs
}
%
\def\deletetokenfromlist#1#2{%
  \def\appended@defcs{}%
  \expandafter\@tfor\expandafter\element@tforcs\expandafter:\expandafter=#2\do{%
    \ifx\element@tforcs#1
    \else
      \expandafter\expandafter\expandafter\def
        \expandafter\expandafter\expandafter\appended@defcs
        \expandafter\expandafter\expandafter{\expandafter\appended@defcs\element@tforcs}%
    \fi
  }%
  \let#2=\appended@defcs
}
\newif\iffoundtoken@bool
\def\addtokentolist#1#2{%
  \foundtoken@boolfalse
  \expandafter\@tfor\expandafter\element@tforcs\expandafter:\expandafter=#2\do{%
    \ifx\element@tforcs#1
      \foundtoken@booltrue
    \fi
  }%
  \iffoundtoken@bool
  \else
    \expandafter\expandafter\expandafter\def
      \expandafter\expandafter\expandafter#2%
      \expandafter\expandafter\expandafter{%
      \expandafter#2#1}%
  \fi
}
%
\newif\iffoundinlist@bool
\def\iffoundinlist#1#2#3#4{%
  \foundinlist@boolfalse
  \expandafter\@tfor\expandafter\tk@tforcs\expandafter:\expandafter=#2\do{%
    \ifx#1\tk@tforcs
      \foundinlist@booltrue
    \fi
  }%
  \iffoundinlist@bool
    #3%
  \else
    #4%
  \fi
}
\def\iffoundinlisttriple#1#2#3#4#5#6{%
  \iffoundinlist{#1}{#2}{%
    #4%
  }{%
    \iffoundinlist{#1}{#3}{%
      #5%
    }{%
      #6%
    }%
  }%
}
\newif\ifgft@bool
\def\getfirsttoken@gfnlf#1#2{%
  \gft@booltrue
  \@tfor\gft@tforcs:=#1\do{%
    \ifgft@bool
      \gft@boolfalse%
      \expandafter\def\expandafter#2\expandafter{\gft@tforcs}%
    \fi
  }%
}
\def\fmlskip{\ }%(skip between formula and logical sign)
\def\lfpv#1{#1}%(predicate/propositional variables)
\def\lflnot#1{%
  \getfirsttoken@gfnlf{#1}{\lfparamR@defcs}%
  \lnot
  \iffoundinlisttriple{\lfparamR@defcs}{\omitlistRlflnot@defcs}{\mndtlistLlflnot@defcs}{#1}{\paren{#1}}{\paren{#1}}
}
