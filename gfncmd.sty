%---行列の集合--------------------------
\newcounter{caseMat@gfncmd}
\setcounter{caseMat@gfncmd}{0}
\newcommand{\useromanMat}{\setcounter{caseMat@gfncmd}{0}}
\newcommand{\useitalicMat}{\setcounter{caseMat@gfncmd}{1}}
\newcommand{\usebmMat}{\setcounter{caseMat@gfncmd}{2}}
\newcommand{\usetimesMat}{\setcounter{caseMat@gfncmd}{3}}
\newcommand{\Mat}[3]{%
  {%
    \ifcase\value{caseMat@gfncmd}%
      \mathrm{M}_{#1, #2}\paren{#3}%
    \or%
      M_{#1, #2}\paren{#3}%
    \or%
      \bm{M}_{#1, #2}\paren{#3}%
    \or%
      #3^{#1\times#2}%
    \fi%
  }%
}
%---冪集合------------------------------
\newcounter{casePowerset@gfncmd}
\setcounter{casePowerset@gfncmd}{0}
\newcommand{\useromanPowerset}{\setcounter{casePowerset@gfncmd}{0}}
\newcommand{\usebinaryPowerset}{\setcounter{casePowerset@gfncmd}{1}}
\newcommand{\usefrakPowerset}{\setcounter{casePowerset@gfncmd}{2}}
\newcommand{\Powerset}[1]{%
  {%
    \ifcase\value{casePowerset@gfncmd}%
      \mathop{\mathrm{Pow}}#1%
    \or%
      2^{#1}%
    \or%
      \mathfrak{P}#1%
    \fi%
  }%
}
\newcommand{\Powersetprn}[1]{%
  {%
    \ifcase\value{casePowerset@gfncmd}%
      \mathop{\mathrm{Pow}}\paren{#1}%
    \or%
      2^{#1}%
    \or%
      \mathop{\mathfrak{P}}\paren{#1}%
    \fi%
  }%
}
%---開区間・閉区間----------------------
\newcommand{\openinterval}[2]{\paren{#1, #2}}
\newcommand{\closedinterval}[2]{\sqbrc{#1, #2}}
\newcommand{\openclosedinterval}[2]{\left(#1, #2\right]}
\newcommand{\closedopeninterval}[2]{\left[#1, #2\right)}
%---函数の定義域と値域------------------
\newcommand{\funcdoms}[3]{#1:{\:}#2\to#3}
