\newcommand{\usemathbfset}{\setcounter{caseset@gfncmd}{0}}
\newcommand{\usebmset}{\setcounter{caseset@gfncmd}{1}}
\newcommand{\usemathbbset}{\setcounter{caseset@gfncmd}{2}}
\newcommand{\setcapital}[1]{%
  {%
    \ifcase\value{caseset@gfncmd}%
      \mathbf{#1}%
    \or%
      \bm{#1}%
    \or%
      \mathbb{#1}%
    \fi%
  }%
}
\newcommand{\setZ}{\setcapital{Z}}
\newcommand{\setN}{\setcapital{N}}
\newcommand{\setQ}{\setcapital{Q}}
\newcommand{\setR}{\setcapital{R}}
