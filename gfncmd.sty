%---半順序 (partial order) -------------
\newcounter{casepord@gfncmd} \setcounter{casepord@gfncmd}{0}
\newcommand{\usepreccurlyeqaspord}{\setcounter{casepord@gfncmd}{0}}
\newcommand{\usepreceqaspord}{\setcounter{casepord@gfncmd}{1}}
\newcommand{\useleqaspord}{\setcounter{casepord@gfncmd}{2}}
\newcommand{\pord}{%
  \mathrel{%
    \ifcase\value{casepord@gfncmd}%
      \mathord{\preccurlyeq}%
    \or%
      \mathord{\preceq}%
    \or%
      \mathord{\leq}%
    \fi%
  }%
}
\newcommand{\join}{\mathbin{\scriptstyle \!\mathord{\lor}}\!} % join
\newcommand{\meet}{\mathbin{\scriptstyle \!\mathord{\land}}\!} % meet
