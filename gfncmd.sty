%===記号論理====================================================================
\newcommand{\deductleft}{\enskip \vdash \enskip}
\newcommand{\deductleftnoassump}{\vdash \enskip}
\newcommand{\deductright}{\enskip \dashv \enskip}
\newcommand{\deductleftright}{\enskip \vdash \dashv \enskip}
%---論理包含 (implimentation) ----------
\newcounter{caseimpl@gfncmd}
\setcounter{caseimpl@gfncmd}{0}
\newcommand{\userightarrowasimpl}{\setcounter{caseimpl@gfncmd}{0}}
\newcommand{\useRightarrowasimpl}{\setcounter{caseimpl@gfncmd}{1}}
\newcommand{\useinvertedCasimpl}{\setcounter{caseimpl@gfncmd}{2}}
\newcommand{\impl}{%
  \mathrel{%
    \ifcase\value{caseimpl@gfncmd}%
      \rightarrow%
    \or%
      \Rightarrow%
    \or%
      \supset%
    \fi%
  }%
}
%---相互論理包含（同値）----------------
\newcounter{caseimplequiv@gfncmd}
\setcounter{caseimplequiv@gfncmd}{0}
\newcommand{\useleftrightarrowasimplequiv}{\setcounter{caseimplequiv@gfncmd}{0}}
\newcommand{\userightleftarrowsasimplequiv}{\setcounter{caseimplequiv@gfncmd}{1}}
\newcommand{\useLeftrightarrowasimplequiv}{\setcounter{caseimplequiv@gfncmd}{2}}
\newcommand{\implequiv}{%
  \mathrel{%
    \ifcase\value{caseimplequiv@gfncmd}%
      \leftrightarrow%
    \or%
      \rightleftarrows% (amssymb.sty)
    \or%
      \Leftrightarrow%
    \fi%
  }%
}
