%---括弧類------------------------------
\newcommand{\dist}[1]{\left\|#1\right\|}
\newcommand{\paren}[1]{\left(#1\right)}
\newcommand{\sqbrc}[1]{\left[#1\right]}
\newcommand{\curlybrc}[1]{\left\{#1\right\}}
\newcommand{\abs}[1]{\left|#1\right|}
\newcommand{\setprn}[1]{\curlybrc{#1}}
    %集合用の括弧
\newcommand{\seqprn}[1]{\paren{#1}}
    %直積の元の括弧
\newcommand{\setprnsep}[2]{\setprn{{\:}#1{\:}|{\:}#2{\:}}}
    %分出公理に基づいて定義される集合用の括弧
%---函数引数用括弧----------------------
\newcounter{casefprn@gfncmd}
\setcounter{casefprn@gfncmd}{0}
\newcommand{\usenormalfprn}{\setcounter{casefprn@gfncmd}{0}}
\newcommand{\usescriptfprn}{\setcounter{casefprn@gfncmd}{1}}
\newcommand{\usesmallfprn}{\setcounter{casefprn@gfncmd}{2}}
\newcommand{\fprn}[1]{%
  \ifcase\value{casefprn@gfncmd}%
    \paren{#1}%
  \or%
    {\scriptstyle\paren{#1}}%
  \or%
    \text{\small$\displaystyle\paren{#1}$}%
  \fi%
}
